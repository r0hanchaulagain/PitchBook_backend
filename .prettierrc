{
  
    "tabWidth": 2,
    "printWidth": 80,
    "useTabs": true,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "proseWrap": "always",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "insertPragma": false,
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
}
